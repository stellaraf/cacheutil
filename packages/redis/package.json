{
  "name": "@stellaraf/cacheutil-redis",
  "version": "2.0.1",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "author": "Matt Love <matt@stellar.tech>",
  "license": "BSD-3-Clause",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": {
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js"
    }
  },
  "files": [
    "dist/",
    "esm/"
  ],
  "dependencies": {
    "@stellaraf/cacheutil-core": "*",
    "ioredis": "^5.2.3"
  },
  "devDependencies": {
    "@stellaraf/eslint-config": "^1.1.4",
    "@stellaraf/jest": "^2.0.0",
    "@types/jest": "^29.0.2",
    "@types/node": "^18.7.18",
    "esbuild": "^0.15.8",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.23.1",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "lint": "eslint --quiet src/**/*",
    "format": "prettier --loglevel warn -c .",
    "typecheck": "tsc --build",
    "build:js": "ts-node-esm build.ts",
    "build:ts": "tsc",
    "build": "pnpm run build:js && pnpm run build:ts",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest --detectOpenHandles"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@stellaraf"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "semi": true
  }
}
